pipeline {
    agent any

   

    stages {
     

        stage('Verify Dagger Installation') {
            steps {
                script {
                    // Check if Dagger is installed correctly
                    sh 'dagger version'
                }
            }
        }

        stage('Git Checkout') {
            steps {
                script {
                    // Run Dagger command
                    sh 'dagger run gitCheckout'
                }
            }
        }

        // Other stages as needed...

    }
}
